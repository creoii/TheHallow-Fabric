{
  "multipart": [
    {
      "when": {
        "axis": "y"
      },
      "apply": {
        "model": "hallows:block/ebony_branch"
      }
    },
    {
      "when": {
        "axis": "x"
      },
      "apply": {
        "model": "hallows:block/ebony_branch",
        "x": 90,
        "y": 90
      }
    },
    {
      "when": {
        "axis": "z"
      },
      "apply": {
        "model": "hallows:block/ebony_branch",
        "x": 90
      }
    }
  ]
}